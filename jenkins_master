pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'slave', parameters: [string(name: 'NAME', defaultValue: 'Alice'), string(name: 'Ages', defaultValue: '21')]
            }
        }
    }
}
